@model IEnumerable<Sitio.Models.ConsultarComisionesPorSocio_Result>
@{
    ViewBag.Title = "Mis Comisiones";
    Layout = "~/Views/Shared/_layout_consulta.cshtml";
}
<h2>Mis Comisiones</h2>

<table class="table">
    <tr>
        <th>
           Id
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Socio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CuentaSocio)
        </th>
        <th>
            Nivel Red
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descricpion)
        </th>
        <th>
            Cliente
        </th>
        <th>
            Comisión
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estatus)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.idComision)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Socio)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.CuentaSocio)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.nivelRed)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Descricpion)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.nombreCliente)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.comisionSocioRed)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Estatus)
    </td>
</tr>
    }
</table>